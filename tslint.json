{
  "defaultSeverity": "error",
  "extends": [
      "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": [
      false
    ],
    "triple-equals":[
      false
    ],
    "no-else-after-return":[
      false
    ],
    "arrow-parens": [
      false
    ],
    "object-curly-spacing": [
      false
    ],
    "ter-indent": [
      false
    ],
    "brace-style": [
      false
    ]
  },
  "rulesDirectory": []
}
