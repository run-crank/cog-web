{
  "name": "cog-web",
  "version": "0.6.0",
  "description": "A generic web browser Cog for Crank",
  "license": "MIT",
  "cog": {
    "name": "automatoninc/web",
    "label": "A Browser",
    "homepage": "https://github.com/run-crank/cog-web",
    "authHelpUrl": ""
  },
  "scripts": {
    "build-docker": "docker build -t automatoninc/web:$npm_package_version -t automatoninc/web:latest .",
    "build-proto": "scripts/build-proto.sh",
    "build-ts": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "check-engine package.json && ts-node src/core/grpc-server.ts",
    "test": "nyc mocha -r ts-node/register src/core/grpc-server.ts test/*.ts test/**/*.ts ",
    "version": "crank cog:readme automatoninc/web && git add README.md"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts",
      "**/*.js",
      "src/core/grpc-server.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "7.1.1",
    "@types/google-protobuf": "^3.2.7",
    "@types/mocha": "^5.2.7",
    "@types/needle": "^2.0.4",
    "@types/node": "^12.0.4",
    "@types/puppeteer": "^1.12.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "check-engine": "^1.8.1",
    "grpc-tools": "^1.9.0",
    "grpc_tools_node_protoc_ts": "^2.5.1",
    "mocha": "^8.0.1",
    "nyc": "^14.1.1",
    "sinon-chai": "^3.3.0",
    "ts-sinon": "^1.0.17",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@run-crank/utilities": "^0.4.5",
    "bluebird": "^3.5.5",
    "google-protobuf": "^3.8.0",
    "grpc": "^1.24.3",
    "lighthouse": "^5.6.0",
    "moment": "^2.29.1",
    "puppeteer": "^1.18.1",
    "puppeteer-cluster": "^0.16.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-recaptcha": "^3.5.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "ts-node": "^8.3.0"
  }
}
