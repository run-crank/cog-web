{
  "name": "cog-web",
  "version": "0.6.0",
  "description": "A generic web browser Cog for Crank",
  "license": "MIT",
  "cog": {
    "name": "stackmoxie/web",
    "label": "A Browser",
    "homepage": "https://github.com/run-crank/cog-web",
    "authHelpUrl": ""
  },
  "scripts": {
    "build-docker": "docker build -t stackmoxie/web:$npm_package_version -t stackmoxie/web:latest .",
    "build-proto": "scripts/build-proto.sh",
    "build-ts": "tsc",
    "lint": "eslint --ext .ts src/client src/steps --quiet",
    "start": "check-engine package.json && ts-node src/core/grpc-server.ts",
    "test": "nyc mocha -r ts-node/register src/core/grpc-server.ts test/*.ts test/**/*.ts ",
    "version": "crank cog:readme stackmoxie/web && git add README.md"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts",
      "**/*.js",
      "src/core/grpc-server.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "7.1.1",
    "@types/google-protobuf": "^3.2.7",
    "@types/mocha": "^5.2.7",
    "@types/needle": "^2.0.4",
    "@types/node": "^12.0.4",
    "@types/puppeteer": "^1.12.4",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "check-engine": "^1.8.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "grpc_tools_node_protoc_ts": "^5.3.1",
    "grpc-tools": "^1.9.0",
    "mocha": "^8.0.1",
    "nyc": "^14.1.1",
    "sinon-chai": "^3.3.0",
    "ts-sinon": "^1.0.17",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.11",
    "@run-crank/utilities": "^0.5.2",
    "bluebird": "^3.5.5",
    "google-protobuf": "^3.8.0",
    "lighthouse": "^9.6.8",
    "moment": "^2.29.1",
    "node-marketo-rest": "^0.7.8",
    "p-retry": "^4.6.1",
    "psl": "^1.8.0",
    "puppeteer": "^20.9.0",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-recaptcha": "^3.5.0",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "ts-node": "^8.3.0"
  }
}
